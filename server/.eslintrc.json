{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["google"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 4],
        "comma-dangle": [
            "error",
            {
                // "arrays": "never",
                // "objects": "never",
                // "imports": "never",
                // "exports": "never",
                "functions": "never"
            }
        ],
        "quotes": [2, "double", "avoid-escape"]
    }
}
